<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.devjones.web.member.dao.MemberDao">

<select id="selectMemberAllList" resultType="com.devjones.web.member.MemberDto">

SELECT
	student_no, name, birth, grade, phone, parents_phone, sail_date
FROM
	student
ORDER BY
	student_no ASC

</select>

</mapper>